pipeline {
    agent any
    stages {
        stage('Database tests and Integration tests') {
            steps {
                build job: 'pip2020-backend-integration-tests/develop', wait: true
            }
        }
        stage('Build and deploy') {
            steps {
                build job: 'pip2020-backend-build-deploy/develop', wait: true, parameters: [
                        booleanParam(name: 'MODIFICATION', value: true)
                ]
            }
        }
    }
}
