pipeline {
    agent any
    stages {
        stage('Invoke Integarion tests') {
            build job: 'pip2020-backend-integration-tests/develop', wait: true
        }
        stage('Build') {
            steps {
                sh 'mvn clean install -DskipTests'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
